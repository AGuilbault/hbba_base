<launch>
    <group ns="hbba">
        <node name="iw" pkg="iw" type="iw" output="screen"/>
        <node name="iw_translator" pkg="iw_translator" type="iw_translator_node"
            output="screen"/>
        <node name="events_generator" pkg="iw" type="events_generator"/>
        <node name="goto_events_observer" 
              pkg="iw_event_generators" 
              type="goto_events_observer">
            <rosparam>
                goal_eps: 0.50
            </rosparam>
        </node>
        <rosparam>
            solver_model:
                solver_log:  false
                solver_sa:   false
                time_limit:   1000
                max_p:        true
        </rosparam>
    </group>
</launch>
